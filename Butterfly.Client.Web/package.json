{
  "name": "butterfly-client",
  "version": "2.0.11",
  "description": "Client library for Butterfly Realtime Web App Server",
  "main": "lib/butterfly-client.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firesharkstudios/butterfly-server-dotnet.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Kent Johnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firesharkstudios/butterfly-server-dotnet/issues"
  },
  "homepage": "https://github.com/firesharkstudios/butterfly-server-dotnet",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.2.1",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "yargs": "^10.0.3"
  }
}
